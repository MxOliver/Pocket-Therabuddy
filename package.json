{
  "name": "pocket-therabuddy",
  "version": "1.0.0",
  "engines": {
    "node": "10.13.0",
    "npm": "6.7.0"
  },
  "description": "Pocket-sized mental health tool-kit application",
  "main": "src/server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node src/server.js",
    "server": "nodemon src/server.js --ignore './client'",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "test": "export NODE_ENV=test&& jasmine",
    "heroku-postbuild": "npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MxOliver/Pocket-Therabuddy.git"
  },
  "author": "Oliver L Coley",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MxOliver/Pocket-Therabuddy/issues"
  },
  "homepage": "https://github.com/MxOliver/Pocket-Therabuddy#readme",
  "devDependencies": {
    "faker": "^4.1.0",
    "jasmine": "^3.3.1",
    "request": "^2.88.0"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "cors": "^2.8.5",
    "d3": "^5.9.2",
    "dotenv": "^7.0.0",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "express-flash": "0.0.2",
    "express-session": "^1.15.6",
    "heroku": "^7.22.9",
    "http-proxy-middleware": "^0.19.1",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.11",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.9.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^5.3.0",
    "sequelize-cli": "^5.4.0"
  }
}
